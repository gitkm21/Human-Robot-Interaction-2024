    def onInput_onStart(self):
        seed()
        memProxy = ALProxy("ALMemory","localhost",9559)
        emote = memProxy.getData("emote")
        if emote =="neutral" : emote= "N"
        elif emote=="angry" : emote="A"
        else : emote="S"

        agent = memProxy.getData("agent")
